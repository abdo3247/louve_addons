<openerp>
    <data>
        <record id="view_res_partner_shift_state_form" model="ir.ui.view">
            <field name="model">res.partner</field>
            <field name="name">res.partner.form.shift_state</field>
            <field name="inherit_id" ref="coop_shift.view_res_partner_shift_form"/>
            <field name="groups_id" eval="[(4, ref('coop_shift_state.group_shift_see_counter'))]"/>
            <field name="arch" type="xml">
                <group name="button" position="before">
                    <group name="state">
                        <field name="state"/>
                        <field name="state_temp"/>
                        <field name="point_standard"/>
                        <field name="point_ftop"/>
                        <field name="shift_delay"/>
                        <field name="date_alert_end"/>
                        <field name="date_delay_end"/>
                        <field name="is_unpayed"/>
                        <field name="is_blocked"/>
                        <field name="shift_type"/>
                    </group>
                </group>
            </field>
        </record>
    </data>
</openerp>