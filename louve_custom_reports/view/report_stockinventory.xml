<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <template id="report_inventory_louve" inherit_id="stock.report_inventory">
            <xpath expr="//table[@class='table table-condensed'][2]/thead/tr//th[5]" position="after">
                <th class="text-right"><strong>Qty1</strong></th>
                <th class="text-right"><strong>Qty2</strong></th>
                <th class="text-right"><strong>Total Qty</strong></th>
            </xpath>
            <xpath expr="//table[@class='table table-condensed'][2]/thead/tr//th[5]" position="replace">
                <th class="text-right"><strong>Theorical Qty</strong></th>
            </xpath>
            <xpath expr="//table[@class='table table-condensed'][2]/thead/tr//th[3]" position="replace"/>
            <xpath expr="//table[@class='table table-condensed'][2]/thead/tr//th[1]" position="replace"/>
            <xpath expr="//table[@class='table table-condensed'][2]/tr//td[5]" position="after">
                <td></td>
                <td></td>
                <td></td>
            </xpath>
            <xpath expr="//table[@class='table table-condensed'][2]/tr//td[3]" position="replace"/>
            <xpath expr="//table[@class='table table-condensed'][2]/tr//td[1]" position="replace"/>
        </template>
    </data>
</openerp>
